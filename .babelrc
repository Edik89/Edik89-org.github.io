{

  "presets": [
    [ "es2015", { "modules": false } ],
    "react",
    "stage-3"
  ],

  "plugins": [
    "transform-class-properties",
    "transform-es2015-template-literals",
    "syntax-dynamic-import"
  ],

  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }

}
